{
  "name": "control",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Thanasis G.",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc --build tsconfig.build.json",
    "lint": "eslint",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts --tsconfig tsconfig.build.json",
    "test:watch": "TS_NODE_PROJECT='./tsconfig.tests.json' mocha -r source-map-support/register -r ts-node/register --extension ts --recursive -w -b tests",
    "test": "TS_NODE_PROJECT='./tsconfig.tests.json' mocha -r ts-node/register --extension ts --recursive --exit tests",
    "migrate:create": "db-migrate create migration-name --config dbfile.js -e default",
    "migrate:up": "db-migrate up --config dbfile.js dbfile.js -e default",
    "migrate:down": "db-migrate down --config dbfile.js dbfile.js -e default",
    "check-types": "tsc --noEmit -p tsconfig.build.json"
  },
  "dependencies": {
    "api-client": "*",
    "dayjs": "^1.11.13",
    "google-auth-library": "^9.14.2",
    "library": "*",
    "lodash": "^4.17.21",
    "logger": "*",
    "microservice": "*",
    "middlewares": "*",
    "sinon": "^19.0.2",
    "source-map-support": "^0.5.21",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.43.3",
    "@types/sinon": "^17.0.3",
    "eslint-config-custom": "*"
  }
}
